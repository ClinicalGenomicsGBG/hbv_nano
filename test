thisdict = {"a": 0.34,"b": 45,"c": 1}
print(thisdict["b"])

#sorted(thisdict.items(), key=lambda x: x[1], reverse=True)
#script to get the values from a dictionary in a sorted order
sorted = {k: v for k, v in sorted(thisdict.items(), key=lambda item: item[1])}
print(sorted)